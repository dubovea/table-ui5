{
	"sap.app": {
		"title": "Тестовое задание",
		"applicationVersion": {
			"version": "0.0.1"
		},
		"id": "tasklist",
		"type": "application"
	},
	"sap.ui": {
		"deviceTypes": {
			"desktop": true,
			"phone": false,
			"tablet": false
		},
		"technology": "UI5",
		"fullWidth": true
	},
	"sap.ui5": {
		"contentDensities": {
			"compact": false,
			"cozy": true
		},
		"dependencies": {
			"minUI5Version": "1.60.30",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.table": {}
			}
		},
		"rootView": {
			"viewName": "tasklist.view.App",
			"type": "XML",
			"async": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "tasklist.i18n.i18n"
				}
			},
			"state": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"busy": true,
					"editable": false,
					"invalidated": false,
					"taskList": {
						"items": [],
						"busy": false
					},
					"filter": {
						"taskTypes": {
							"selectedKey": "",
							"items": [
								{
									"Id": "1",
									"Name": "Разработка"
								},
								{
									"Id": "2",
									"Name": "Тестирование"
								},
								{
									"Id": "3",
									"Name": "Проектирование"
								}
							]
						},
						"users": {
							"id": "",
							"value": "",
							"error": "",
							"items": [
								{
									"Id": "1",
									"Name": "Дубов Эдуард Александрович"
								},
								{
									"Id": "2",
									"Name": "Манин Михаил Александрович"
								},
								{
									"Id": "3",
									"Name": "Манин Александр Михайлович"
								}
							]
						},
						"dateBegin": null,
						"dateEnd": null
					}
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			],
			"js": []
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "tasklist.view",
				"controlId": "app",
				"bypassed": {
					"target": [
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": ":editable:",
					"name": "taskSelect",
					"target": [
						"taskSelect"
					]
				},
				{
					"pattern": "task/view",
					"name": "taskView",
					"target": [
						"taskView"
					]
				},
				{
					"pattern": "task/edit",
					"name": "taskEdit",
					"target": [
						"taskEdit"
					]
				}
			],
			"targets": {
				"notFound": {
					"viewName": "NotFound",
					"viewLevel": 1,
					"controlAggregation": "pages"
				},
				"taskSelect": {
					"viewType": "XML",
					"viewName": "Tasks.Select",
					"viewLevel": 1,
					"controlAggregation": "pages"
				},
				"taskView": {
					"viewType": "XML",
					"viewName": "Tasks.Overview",
					"viewLevel": 1,
					"controlAggregation": "pages"
				},
				"taskEdit": {
					"viewType": "XML",
					"viewName": "Tasks.Editing",
					"viewLevel": 1,
					"controlAggregation": "pages"
				}
			}
		}
	}
}